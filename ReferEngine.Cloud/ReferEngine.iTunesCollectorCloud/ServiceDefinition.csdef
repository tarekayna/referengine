<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="ReferEngine.iTunesCollectorCloud" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2013-03.2.0">
  <WorkerRole name="ReferEngine.iTunesCollector" vmsize="Small">
    <Imports>
      <Import moduleName="Diagnostics" />
      <Import moduleName="RemoteAccess" />
      <Import moduleName="RemoteForwarder" />
    </Imports>
    <ConfigurationSettings>
      <Setting name="CurrentServiceConfiguration" />
      <Setting name="TracerConnectionString" />
    </ConfigurationSettings>
    <LocalResources>
      <LocalStorage name="localStore" sizeInMB="204800" cleanOnRoleRecycle="false" />
    </LocalResources>
  </WorkerRole>
</ServiceDefinition>