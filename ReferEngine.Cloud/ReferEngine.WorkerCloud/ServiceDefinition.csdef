<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="ReferEngine.WorkerCloud" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2012-10.1.8">
  <WorkerRole name="ReferEngine.WorkerCloud.WinApps" vmsize="ExtraSmall">
    <Imports>
      <Import moduleName="Diagnostics" />
      <Import moduleName="RemoteAccess" />
    </Imports>
    <ConfigurationSettings>
      <Setting name="CurrentServiceConfiguration" />
    </ConfigurationSettings>
  </WorkerRole>
  <WorkerRole name="ReferEngine.WorkerCloud.iOSApps" vmsize="ExtraSmall">
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
  </WorkerRole>
</ServiceDefinition>