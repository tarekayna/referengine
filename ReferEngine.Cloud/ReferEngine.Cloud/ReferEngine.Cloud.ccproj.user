    <ActivePublishProfile>referengine.com staging</ActivePublishProfile>
    <DeploymentProfilingMode>Instrumentation</DeploymentProfilingMode>
    <DeploymentEnableTierInteractionProfiling>False</DeploymentEnableTierInteractionProfiling>
