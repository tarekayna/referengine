<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="ReferEngine.PingerCloud" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2013-03.2.0">
  <WorkerRole name="ReferEngine.Pinger" vmsize="ExtraSmall">
    <Imports><Import moduleName="Diagnostics" /></Imports>
    <ConfigurationSettings>
      <Setting name="CurrentServiceConfiguration" />
      <Setting name="TracerConnectionString" />
    <Setting name="Microsoft.WindowsAzure.Plugins.Caching.ClientDiagnosticLevel" /></ConfigurationSettings>
  <Startup priority="-2"><Task commandLine="Microsoft.WindowsAzure.Caching\ClientPerfCountersInstaller.exe install" executionContext="elevated" taskType="simple" /></Startup></WorkerRole>
</ServiceDefinition>